<div class="module header-module component" data-component-term="search_header">
  <div class="flex-module">
    <form class="search-header" action="/search" method="GET">
      <table class="flex-table">
        <tbody>
          <tr>
            <td class="flex-table-primary">
              <div class="flex-table-form">
                <input class="flex-table-input search-query" name="q" type="text" placeholder="Search">
              </div>
            </td>
            <td class="flex-table-secondary">
              <div class="btn-group flex-table-btn">
                <a class="btn search-btn" tabindex="0">Search</a>
              </div>
            </td>
          </tr>
        </tbody>
      </table>
    </form>
    <h2>Search results</h2>
  </div>
</div>
<div class="content-main">
  <div class="content-header">
    <div class="header-inner">
      <h2><span class="stream-title"></span></h2>
    </div>
  </div>
  <div id="stream-items">
  </div>
  <div class="stream-loading">
    <div class="stream-end-inner">
      <!-- <span class="spinner" title="Loading..."></span> -->
    </div>
  </div>
</div>
